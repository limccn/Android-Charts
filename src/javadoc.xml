<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<project basedir="." default="javadoc">
	<property name="android_sdk_home" value="../../android-sdk-macosx/" />
	<property name="javadoc_dest_dir" value="../doc/javadoc" />
	<target name="javadoc">
		<javadoc additionalparam="-locale en_US -encoding utf-8 -charset utf-8"
			classpath="${android_sdk_home}/tools/support/annotations.jar;${android_sdk_home}/platforms/android-7/android.jar"
			packagenames="cn.limc.androidcharts.event,cn.limc.androidcharts.entity,cn.limc.androidcharts.view"
			destdir="${javadoc_dest_dir}"
			nodeprecated="false" access="public" author="true"
			nodeprecatedlist="false" noindex="false" nonavbar="false" notree="false"
			source="1.5" sourcepath="src" splitindex="true" use="true" version="true" />
	</target>
</project>
